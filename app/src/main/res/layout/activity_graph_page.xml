<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kleog.fitnessapp.GraphPage">

    <com.jjoe64.graphview.GraphView
        android:id="@+id/graph"
        android:layout_width="135dp"
        android:layout_height="135dp"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="44dp"
        app:layout_constraintTop_toBottomOf="@+id/graphView"
        android:layout_marginRight="30dp"
        app:layout_constraintRight_toRightOf="parent" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/graphView4"
        android:layout_width="135dp"
        android:layout_height="135dp"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="44dp"
        app:layout_constraintTop_toBottomOf="@+id/graphView3"
        app:layout_constraintRight_toLeftOf="@+id/graph"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/graphView"
        android:layout_width="135dp"
        android:layout_height="135dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="39dp"
        android:layout_marginRight="30dp"
        app:layout_constraintRight_toRightOf="parent" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/graphView3"
        android:layout_width="135dp"
        android:layout_height="135dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="39dp"
        app:layout_constraintRight_toLeftOf="@+id/graphView"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:text="Graph One"
        app:layout_constraintBottom_toTopOf="@+id/graphView3"
        android:layout_marginLeft="42dp"
        app:layout_constraintLeft_toLeftOf="parent" />


</android.support.constraint.ConstraintLayout>

